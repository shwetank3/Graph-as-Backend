    <label>Label</label><br>
    <input type="text" name="SubNode_label"><br>
    <label>Properties</label><br>
    <input type="text" name="SubNode_pro">
    <br><br>
    <input type="submit" value="Submit">
</form>

<h3>Delete Node</h3>
<form method="post" action="/node/del">
    <label>Label</label><br>
    <input type="text" name="Node_label">
    <br><br>
    <input type="submit" value="Submit">
</form>



